.parallax
    position absolute
    top 0
    right 0
    width 900px
    height 765px
    z-index -10
    overflow hidden
    img
        display block
        max-width 100%
    +adapt(1440px)
        width 700px
        height 620px
    +adapt(1200px)
        width 600px
    +adapt(992px)
        display none

.parallax-mob-img
    display block
    max-width 100%

.layer
    display block
    max-width 100%
    +adapt(1440px)
        height 100%
.parallax-layer1,
.parallax-layer2,
.parallax-layer3,
.parallax-layer4
    position absolute
    top 0
    left 0
    width 100%
    height 100%
    z-index -10
.parallax-layer1
    z-index 0
.parallax-layer2
    z-index 10
.parallax-layer3
    z-index 20
.parallax-layer4
    z-index 15  
.parallax__doctor-img1
    position absolute
    z-index 10
    transform translate(0, 700px)
    left 35%
    width 300px
    height 400px
    animation visible1 1s 1s ease-in-out forwards
    z-index 50
    +adapt(1440px)
        width 200px
        height 300px
    +adapt(1200px)
        width 150px
        height 250px
        transform translate(0, 600px)
.parallax__doctor-img2
    position absolute
    z-index 10
    transform translate(900px, 290px)
    width 300px
    height 350px
    z-index 40 
    +adapt(1440px)
        width 200px
        height 300px
    +adapt(1200px)
        width 180px
        height 280px

    
.parallax__doctor-img3
    position absolute
    z-index 10
    transform translate(180px, 780px)
    width 250px
    height 300px
    z-index 40
    
    +adapt(1440px)
        width 150px
        height 250px
.parallax__doctor-img4
    position absolute
    z-index 10
    transform translate(430px, -385px)
    width 200px
    height 250px
    z-index 30
    animation visible4 1s 2s ease-in-out forwards
    +adapt(1440px)
        width 150px
        height 250px
.parallax__doctor-img5
    position absolute
    transform translate(230px, -385px)
    width 230px
    height 270px
    z-index 30
    animation visible5 1.5s 1.9s ease-in-out forwards
    
@media screen and (min-width: 1201px) 
    .parallax__doctor-img2
        animation visible2 1s 1.5s ease-in-out forwards
    .parallax__doctor-img3
        animation visible3 1s 1.5s ease-in-out forwards
    .parallax__doctor-img5
        animation visible5 1s 1.5s ease-in-out forwards
@media screen and (max-width: 1201px) 
    .parallax__doctor-img2
        animation visible2-def 1s 1.5s ease-in-out forwards
    .parallax__doctor-img3
        animation visible3-def 1s 1.5s ease-in-out forwards
    .parallax__doctor-img5
        animation visible5-def 1s 1.5s ease-in-out forwards
@keyframes visible1
    0%,10%
        transform translate(0, 700px)
    100%
        transform translate(0, 390px)
        
@keyframes visible2
    0%
        transform translate(900px, 290px)
    100%
        transform translate(450px, 290px)
@keyframes visible2-def
    0%
        transform translate(900px, 290px)
    100%
        transform translate(350px, 290px)


@keyframes visible3
    0%
        transform translate(-200px, 280px)
    100%
        transform translate(180px, 280px)
@keyframes visible3-def
    0%
        transform translate(-200px, 280px)
    100%
        transform translate(120px, 220px)
@keyframes visible4
    0%
        transform translate(430px, -385px)
    100%
        transform translate(430px, 185px)
@keyframes visible5
    0%
        transform translate(230px, -385px)
    100%
        transform translate(230px, 140px)
@keyframes visible5-def
    0%
        transform translate(230px, -385px)
    100%
        transform translate(180px, 70px)
.parallax-parts1,
.parallax-parts2,
.parallax-parts3,
.parallax-parts4,
.parallax-parts5,
.parallax-parts6,
.parallax-parts7,
.parallax-parts8,
.parallax-parts9,
.parallax-parts10,
.parallax-parts11
    position absolute
    width 50px
    opacity 0
    animation parts 2s 3s ease-in-out forwards
.parallax-parts1
    bottom 100px
    left 100px
.parallax-parts2
    top 10px
    left 50%
.parallax-parts3
    top 30%
    right 90px
    
.parallax-parts4
    top 30%
    left 74px
.parallax-parts5
    top 24%
    left 625px
.parallax-parts6
    top 100px
    left 400px
.parallax-parts7
    bottom 47px
    left 100px
.parallax-parts8
    bottom 90px
    left 190px
.parallax-parts9
    bottom 160px
    left 170px
.parallax-parts10
    top 37%
    left 675px
.parallax-parts11
    top 47%
    left 665px
@keyframes parts
    0%
        opacity 0
    100%
        opacity 1